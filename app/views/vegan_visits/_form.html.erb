
<%= simple_form_for(@vegan_visit) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :url %>
    <%= f.input :fullpath %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
