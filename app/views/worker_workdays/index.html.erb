<p id="notice"><%= notice %></p>

<h1>Worker Workdays</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @worker_workdays.each do |worker_workday| %>
      <tr>
        <td><%= link_to 'Show', worker_workday %></td>
        <td><%= link_to 'Edit', edit_worker_workday_path(worker_workday) %></td>
        <td><%= link_to 'Destroy', worker_workday, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Worker Workday', new_worker_workday_path %>
